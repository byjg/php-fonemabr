{
  "name": "byjg/fonemabr",
  "license": "MIT",
  "description": "O Fonema BR tem por objetivo criar uma simplificação de palavras de tal forma que erros de ortografia e vogais não interfiram na busca",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1 <8.4",
    "byjg/convert": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "vimeo/psalm": "^5.9"
  },
  "autoload": {
    "psr-4": {
      "ByJG\\WordProcess\\": "src/"
    }
  },
  "autoload-dev": {
      "psr-4": {
      "Tests\\": "tests/"
      }
  }
}
